  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "turbo": "^2.0.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0"
    ,"@types/node": "^20.0.0"
  }
}
