{
  "name": "separate-tracks",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "rm -rf node_modules && find . -type d -name node_modules -prune -exec rm -rf '{}' + && rm -rf pnpm-lock.yaml",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down"
    ,"docker:up:detached": "docker compose up --build -d"
    ,"docker:logs": "docker compose logs -f"
    ,"docker:logs:api": "docker compose logs -f api"
    ,"docker:logs:web": "docker compose logs -f web"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "turbo": "^2.0.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0"
    ,"@types/node": "^20.0.0"
  }
}
